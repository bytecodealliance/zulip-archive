    {
        "content": "<p>alexcrichton merged <a href=\"https://github.com/bytecodealliance/wasmtime/pull/12235\">PR #12235</a>.</p>",
        "id": 566390434,
        "sender_full_name": "Wasmtime GitHub notifications bot",
